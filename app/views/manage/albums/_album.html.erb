<tr id="<%= dom_id(album.id) %>">
	<td><%= link_to album.name, manage_album_photos_path(album) %></td>
	<td><%= album.photos.count %></td>
	<td><%= album.comments_count %></td>
	<td><%= album.created_at.strftime('%d-%m-%Y') %></td>
	<td>
		<%= link_to 'UBB', ubb_manage_album_path(album) %> - 
		
		<%= link_to 'edit', edit_manage_album_path(album) %>
	</td>
</tr>